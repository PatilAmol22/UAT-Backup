      ISCHANGED({!myVariable_current.Risk_Category__c}),
      ISCHANGED({!myVariable_current.Rec_Brand__c}),
      ISCHANGED({!myVariable_current.Churn_Type__c}),
      ISCHANGED({!myVariable_current.Type}),
      ISCHANGED({!myVariable_current.Month__c}),
      ISCHANGED({!myVariable_current.Year__c}),
      ISCHANGED({!myVariable_current.AccountId} )
)
)</expression>
    </formulas>
