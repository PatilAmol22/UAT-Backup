    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>&apos;OPP-&apos; + 
IF( ISPICKVAL([Opportunity].Type, &apos;Churn&apos;) , IF( ISPICKVAL([Opportunity].Churn_Type__c, &apos;Hard Churn&apos;) , &apos;Hard-Churn-&apos;, &apos;Soft-Churn-&apos;), &apos;Cross-sell-&apos;) +  
[Opportunity].Portfolio__c + &apos;-&apos; +
IF( ISPICKVAL([Opportunity].Type, &apos;Churn&apos;) , TEXT([Opportunity].Risk_Category__c ) ,[Opportunity].Rec_Brand__c  ) + &apos;-&apos; + 
[Opportunity].Account.SAP_Customer_Code__c + &apos;-&apos; + 
[Opportunity].Month__c + &apos;-&apos; + 
[Opportunity].Year__c </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_7514750312</name>
        <dataType>String</dataType>
        <expression>&apos;OPP-&apos; + 
IF( ISPICKVAL({!myVariable_current.Type}, &apos;Churn&apos;) , IF( ISPICKVAL({!myVariable_current.Churn_Type__c}, &apos;Hard Churn&apos;) , &apos;Hard-Churn-&apos;, &apos;Soft-Churn-&apos;), &apos;Cross-sell-&apos;) +  
{!myVariable_current.Portfolio__c} + &apos;-&apos; +
IF( ISPICKVAL({!myVariable_current.Type}, &apos;Churn&apos;) , TEXT({!myVariable_current.Risk_Category__c} ) ,{!myVariable_current.Rec_Brand__c}  ) + &apos;-&apos; + 
{!myVariable_current.Account.SAP_Customer_Code__c} + &apos;-&apos; + 
{!myVariable_current.Month__c} + &apos;-&apos; + 
{!myVariable_current.Year__c}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
   [Opportunity].RecordType.DeveloperName = &apos;Churn_Cross_Sell&apos;,
   OR(
      ISNEW(),
      ISCHANGED([Opportunity].Name),
      ISCHANGED([Opportunity].Portfolio__c),
      ISCHANGED([Opportunity].Risk_Category__c),
      ISCHANGED([Opportunity].Rec_Brand__c),
      ISCHANGED([Opportunity].Churn_Type__c),
      ISCHANGED([Opportunity].Type),
      ISCHANGED([Opportunity].Month__c),
      ISCHANGED([Opportunity].Year__c),
      ISCHANGED([Opportunity].AccountId )
)
) </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
   {!myVariable_current.RecordType.DeveloperName} = &apos;Churn_Cross_Sell&apos;,
