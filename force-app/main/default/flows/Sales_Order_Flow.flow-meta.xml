    <decisions>
        <name>Check_Values</name>
        <label>Check Values</label>
        <locationX>390</locationX>
        <locationY>96</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Sales_Org_India_ALS</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>AccountId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SalesOrgCode</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>1410</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SalesOrgCode</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>1510</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Look_up_on_Sales_Area</targetReference>
            </connector>
            <label>Sales Org India/ALS</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_UGDN_found</name>
        <label>If UGDN found</label>
        <locationX>390</locationX>
        <locationY>305</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>UGDN_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UGDN_Number</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SAP_Password</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Sales_Order</targetReference>
            </connector>
            <label>UGDN found</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_User_is_found</name>
        <label>If User is found</label>
        <locationX>593</locationX>
        <locationY>197</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AccountOwnerInSalesArea</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lookup_on_user</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>for setting UGDN and Password
