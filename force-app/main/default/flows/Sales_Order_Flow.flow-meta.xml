            <value>
                <elementReference>SalesOrderId</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>SAP_Password__c</field>
            <value>
                <elementReference>SAP_Password</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>UGDN__c</field>
            <value>
                <elementReference>UGDN_Number</elementReference>
            </value>
        </inputAssignments>
        <object>Sales_Order__c</object>
    </recordUpdates>
