    <recordLookups>
        <name>Look_up_on_Sales_Area</name>
        <label>Look up on Sales Area</label>
        <locationX>583</locationX>
        <locationY>95</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>If_User_is_found</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Distributor__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>AccountId</elementReference>
            </value>
        </filters>
        <filters>
            <field>Sales_Org_Code__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>SalesOrgCode</elementReference>
            </value>
        </filters>
        <object>DistributorSalesAreaMapping__c</object>
        <outputAssignments>
            <assignToReference>AccountOwnerInSalesArea</assignToReference>
            <field>AccountOwner__c</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <name>Lookup_on_user</name>
        <label>Lookup on user</label>
        <locationX>384</locationX>
        <locationY>194</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>If_UGDN_found</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>AccountOwnerInSalesArea</elementReference>
            </value>
        </filters>
        <object>User</object>
        <outputAssignments>
            <assignToReference>SAP_Password</assignToReference>
            <field>SAP_User_Password__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>UGDN_Number</assignToReference>
            <field>UGDN_Number__c</field>
        </outputAssignments>
    </recordLookups>
