                                    <center> {!v.MessageBrand}</center>
                                    <aura:iteration items="{!v.listOfSearchRecordsBrand}" var="singleRec">
                                        <c:customLookupResult oRecord="{!singleRec}"/>
                                    </aura:iteration>
                                </ul>
                                
