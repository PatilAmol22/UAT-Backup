                                <lightning:button variant="base" aura:id="openBrnd" label="{!$Label.c.New_Brand}" title="Add New Brand" onclick="{!c.openBrandModal}" />
                            </div>
                        </div>
                    </div>
                </div>
                
                <div style="clear:both;"></div>
                
