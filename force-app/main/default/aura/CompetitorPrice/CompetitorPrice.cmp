                                    <lightning:spinner class="slds-hide" variant="brand" size="small" aura:id="mySpinnerCompany"/>
                                    <center> {!v.MessageCompany}</center>
                                    <aura:iteration items="{!v.listOfSearchRecordsCompany}" var="singleRec">
