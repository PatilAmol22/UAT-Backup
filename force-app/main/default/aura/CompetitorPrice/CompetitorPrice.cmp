                                <aura:if isTrue="{!v.cmpnyOpt}">
                                    <option value="{!v.selectedCmpny.id}">{!v.selectedCmpny.Name}</option>
                                    <aura:set attribute="else">
                                        <option value="">Choose one...</option>
                                    </aura:set> 
