    <aura:handler name="oSelectedRecordEventBrand" event="c:selectedsObjectRecordEvent" action="{!c.handleComponentEventBrand}"/>
    <aura:handler name="oSelectedRecordEventCompany" event="c:selectedsObjectRecordEvent" action="{!c.handleComponentEventCompany}"/>
    <aura:handler name="oSelectedRecordEventFormulation" event="c:selectedsObjectRecordEvent" action="{!c.handleComponentEventFormulation}"/>
    <aura:handler name="oSelectedRecordEventPackSize" event="c:selectedsObjectRecordEvent" action="{!c.handleComponentEventPackSize}"/>
    <aura:handler name="oSelectedRecordEventCrop" event="c:selectedsObjectRecordEvent" action="{!c.handleComponentEventCrop}"/>
    <aura:handler name="oSelectedRecordEventPest" event="c:selectedsObjectRecordEvent" action="{!c.handleComponentEventPest}"/>
    
    
    
    
