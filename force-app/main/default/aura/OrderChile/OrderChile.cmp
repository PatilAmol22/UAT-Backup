                        <td class="slds-truncate" data-label="{!$Label.c.Total_Discount}">
                            <lightning:formattedNumber value="{!v.totalDisc}" format="#,###,##0.00"/> 
                        </td>      
                    </tr>
                    <aura:if isTrue = "{!v.newSalesOrder.CurrencyIsoCode=='CLP'}">
