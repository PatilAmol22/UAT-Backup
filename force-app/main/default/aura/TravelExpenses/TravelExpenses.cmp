<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,lightning:actionOverride,force:lightningQuickAction,force:hasRecordId" 
                controller="TravelExpensesController"
                access="global">
    
    
    <!--Init Component On Load-->
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <!--End-->
    
    <!--Init Component On Load-->
    <aura:attribute name="selectedMTE" type="String" /><!--UPL-2-I386: Modified by:Ankita Saxena Changes in Travel request  for Indonesia-->
    <aura:attribute name="delete_header" type="String" default="Travel Expense Report"/> 
    <aura:attribute name="month_header" type="String" default="Travel Expense Report"/> 
    <aura:attribute name="day_header" type="String" default="Travel Expense Report"/>
    <aura:attribute name="todayDate" type="Date" access="public" />
    <aura:attribute name="month" type="Boolean" default="true"/>
    <aura:attribute name="TravelExpensesList" type="Expense_Item__c[]" access="public" />
    <aura:attribute name="TravelExpenseMonthList" type="Monthly_Travel_Expense__c[]" />
    <aura:attribute name="TravelExpenseDayList" type="Expense_Item__c[]" />
    <aura:attribute name="TravelExpenseDayListEdit" type="Expense_Item__c[]" />
    <aura:attribute name="MonthList" type="String[]"  />
    <aura:attribute name="search" type="String"/>  
    <aura:attribute name="mtd" type="String"/> 
    <aura:attribute name="yr" type="String"/>
    <aura:attribute name="last_SearchText" type="String" access="private" />
    <aura:attribute name="recordId" type="Id" />
    
    <aura:attribute name="arrowDirection" type="string" default="arrowup" description="Use for change arrow sign direction on header based on click"/>  
    <aura:attribute name="isAsc" type="boolean" default="true" description="boolean flag for pass sorting condition to apex class"/>   
    <aura:attribute name="selectedTabsoft" type="string" default="Name" description="Use for show/hide arraow sign on header based on conditions"/>  
    <aura:attribute name="page" default="1" type="integer" access="public" description="using for store page Number"/>  
    <aura:attribute name="pages" default="0" type="integer" access="public" description="using for store All Pages page Number"/>  
    <aura:attribute name="total" default="0" type="integer" access="public" description="total records count store "/> 
    
    <aura:attribute name="total_amount" type="Decimal" default="0.0"/>
    
    <!--End -->
    <!-- Enter values from form --> 
    <aura:attribute name="travelexpense" type="Expense_Item__c"
                    default="{'sobjectType': 'Expense_Item__c',
                             'Date__c': '',
                             'Item__c': '',
                             'Amount__c': '',
                             'Purpose__c': '',
                             'Location__c':'',
                             'Remarks__c':''}"/>
    
    <aura:attribute name="travelexpense_new" type="Expense_Item__c"
                    default="{'sobjectType': 'Expense_Item__c',
                             'Date__c': '',
                             'Item__c': '',
                             'Amount__c': '',
                             'Purpose__c': '',
                             'Location__c':'',
                             'Remarks__c':''}"/>
    
    <!-- Int type number -->    
    <aura:attribute name="amount" type="integer" default="0"/>
    <aura:attribute name="totAmount" type="Decimal" default="0.0"/>
    <!-- End -->
    
    <!-- Int type number -->    
    <aura:attribute name="rowIndex" type="String"/>
    
    <!-- End -->
    
    <!--Removing Item from iteraion list -->
    <aura:handler name="RemoveItemEvent" event="c:RemoveItemEvent" action="{!c.removeDeletedRow}"/> 
    
    <!-- End -->
    
    <aura:attribute name="allocationList" type="List" access="public"/>
    <aura:attribute name="itemList" type="List" access="public"/>
    <aura:attribute name="liquidationList" type="List" access="public"/>
    
    <aura:attribute name="isOpen" type="boolean" default="false"/>
    <aura:attribute name="isOpenMonth" type="boolean" default="false"/>
    <aura:attribute name="isOpenError" type="boolean" default="false"/>
    <aura:attribute name="isEditOpenMonth" type="boolean" default="false"/>
    <aura:attribute name="isExistingDayOpen" type="boolean" default="false"/>
    <aura:attribute name="isOpenDelete" type="boolean" default="false"/>
    <aura:attribute name="isOpenDeleteDay" type="boolean" default="false"/>
    <aura:attribute name="isNSM" type="boolean" default="true"/>
    <aura:attribute name="isNMM" type="boolean" default="true"/>
    <aura:attribute name="isRM" type="boolean" default="true"/>
    <aura:attribute name="isAddNEw" type="boolean" default="false"/>
    
    <!--Modal Popup for Create New -->
    <div class="slds-m-around--xx-large">    
        <!--Use aura:if tag to display Model Box, on the bese of conditions. [isOpen boolean attribute] -->   
        <aura:if isTrue="{!v.isOpen}">
            
            <!--###### MODAL BOX Start From Here ######--> 
            <div role="dialog" aura:id="Modalbox" aria-labelledby="header99" class="slds-modal slds-fade-in-open" style="margin-top:120px;"> <!--  style="margin-top:120px;"-->
                <div class="slds-modal__container">
                    <!-- ###### MODAL BOX HEADER Part Start From Here ######-->
                    <div class="slds-modal__header">
                        <button class="slds-button slds-modal__close slds-button--icon-inverse" title="Close" onclick="{!c.closeModel}">
                            X
                            <span class="slds-assistive-text">Close</span>
                        </button>
                        <h2 id="header99" class="slds-text-heading--medium" style="font-weight:bold;">
                            New Activity
                        </h2>
                    </div>
                    <!--###### MODAL BOX BODY Part Start From Here ######-->
                    <div class="slds-modal__content slds-p-around--medium">
                        
                        <div class="slds">
                            <div class="slds-grid slds-wrap slds-grid--pull-padded" style="background:#fff; margin:0px; padding:0 0 10px 0; border:0px solid #cce4f8;">
                                
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        
                                        <!-- For todays date -->
                                        <!--<div class="slds-form-element__control">
                                            <ui:inputDate aura:id="date" 
                                                          label="Select a date"
                                                          displayDatePicker="true" 
                                                          value="{!v.todayDate}"/>
                                        </div>-->
                                        <div class="slds-form-element__control">
                                            
                                            <ui:inputDate class="slds-input" 
                                                          aura:id="dateId" 
                                                          format="dd/MM/yyyy" 
                                                          label="Date"
                                                          displayDatePicker="true" 
                                                          value="{!v.travelexpense.Date__c}" 
                                                          required="true" 
                                                          disabled="false" />             
                                        </div>
                                        
                                        
                                    </div>
                                </div>
                                
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <ui:inputSelect class="slds-input" 
                                                            label="Items" 
                                                            aura:id="items" 
                                                            value="{!v.travelexpense.Item__c}" 
                                                            change="{!c.onPicklistChange}"
                                                            required="true" 
                                                            disabled="false"
                                                            />     
                                        </div>
                                        
                                    </div>
                                </div>
                                
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <!--<lightning:input type="text" label="No Of Farmers Invited" name="date" required="true"/>-->
                                            <ui:inputNumber class="slds-input" 
                                                            aura:id="amount" 
                                                            label="Amount" 
                                                            format="##.00"
                                                            blur="{!c.onChange}"
                                                            value="{!v.travelexpense.Amount__c}"
                                                            required="true" />
                                            <!--input type="number" aura:id="amount"
                                                   name="Amount" 
                                                   label="Amount"
                                                   min="0"
                                                   class="slds-input"
                                                   required="true"
                                                   labelClass="slds-form-element__label"
                                                   value="{!v.travelexpense.Amount__c}"/>-->
                                        </div>
                                    </div>
                                </div>
                                
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <!--<lightning:input type="text" label="No Of R1 Invited" name="date" required="true"/>-->
                                            <ui:inputText class="slds-input" 
                                                          aura:id="purpose" 
                                                          label="Purpose" 
                                                          value="{!v.travelexpense.Purpose__c}" 
                                                          required="true" />         
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <!--<lightning:input type="text" label="No Of R1 Invited" name="date" required="true"/>-->
                                            <ui:inputText class="slds-input" 
                                                          aura:id="location" 
                                                          label="Location" 
                                                          value="{!v.travelexpense.Location__c}" 
                                                          
                                                          required="true" />         
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <!--<lightning:input type="text" label="No Of R1 Invited" name="date" required="true"/>-->
                                            <ui:inputText class="slds-input" 
                                                          aura:id="remark" 
                                                          label="Remark" 
                                                          value="{!v.travelexpense.Remarks__c}" 
                                                          
                                                          required="true" />         
                                        </div>
                                    </div>
                                </div>
                                <div style="display:none;">
                                    <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2" style="margin-top:12px;">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <!--<lightning:input type="text" label="No Of R1 Invited" name="date" required="true"/>-->
                                                <ui:inputText class="slds-input" 
                                                              aura:id="monthId" 
                                                              value="{!v.travelexpense.Month__c}"  />         
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-p-horizontal--small slds-size--1-of-1" style="margin-top:30px;">
                                    
                                    <div class="slds-form-element" >
                                        <div style="float:right;">
                                            
                                            
                                            <div class="slds-form-element__control"  >
                                                
                                                <button class="slds-button slds-button--brand" onclick="{!c.onAdd}">
                                                    Add Items
                                                </button>
                                            </div >
                                        </div> 
                                    </div>
                                </div>
                            </div>
                        </div>           
                        
                        <div style="clear:both"></div>              
                        
                        <h3 class="slds-section-title--divider slds-p-horizontal--small slds-size--1-of-1" style="margin-top:10px; background:#769ed9; color:#fff; font-weight:bold;">
                            <div style="float:left; display:inline; margin-top:5px; padding-right:20px; font-weight:bold;">
                                New Travel Expense</div>
                            <div style="float:right;">
                                
                            </div> 
                            <div style="clear:both;"></div>
                        </h3>               
                        
                        
                        <div class="slds-scrollable_x" width="100%" style="margin-bottom:0;">
                            <table width="100%" class="slds-table slds-table--bordered">
                                <thead>
                                    <tr class="slds-text-heading--label">
                                        
                                        <th class="slds-is-sortable" scope="col">
                                            <div class="slds-truncate"><b>Date</b></div>
                                        </th>
                                        <th class="slds-is-sortable" scope="col">
                                            <div class="slds-truncate"><b>Items</b></div>
                                        </th>
                                        <th class="slds-is-sortable" scope="col">
                                            <div class="slds-truncate"><b>Amount</b></div>
                                        </th>
                                        <th class="slds-is-sortable" scope="col">
                                            <div class="slds-truncate"><b>Purpose</b></div>
                                        </th>
                                        <th class="slds-is-sortable" scope="col">
                                            <div class="slds-truncate"><b>Location</b></div>
                                        </th>   
                                        <th class="slds-is-sortable" scope="col">
                                            <div class="slds-truncate"><b>Remark</b></div>
                                        </th>
                                        <th class="slds-cell-shrink"></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <!-- Iteration of expense list item -->   
                                    <!--    <p class=".slds-theme_success"> -->
                                    <aura:iteration items="{!v.TravelExpensesList}" var="TravelExpensesList" indexVar="index" > 
                                        <c:TravelExpenseItem TravelExpenses="{!TravelExpensesList}" rowIndex="{!index}"></c:TravelExpenseItem>
                                    </aura:iteration> 
                                    
                                </tbody>
                            </table>
                        </div>
                        <div class="slds-section-title--divider slds-p-horizontal--small slds-size--1-of-1" style="background:#fff;"> 
                            <div class="slds-box slds-box_small slds-float_right" style="background:#eee; font-weight:bold; color:#000; margin-bottom:5px; margin-right:0px;">
                                TOTAL : <ui:outputNumber aura:id="totalAmount"
                                                         value="{!v.totAmount}" 
                                                         format="#,###,##0.00"/>
                                
                            </div>
                        </div>
                        
                        <div style="clear:both;"></div>
                        <div class="slds-section-title--divider slds-p-horizontal--small slds-size--1-of-1" style="background:#fff;"> 
                            
                        </div>
                        
                        
                    </div>
                    <!--###### MODAL BOX FOOTER Part Start From Here ######-->
                    <div class="slds-modal__footer">
                        <button class="slds-button slds-button--neutral" onclick="{!c.closeModel}" >Cancel</button>
                        
                        <button class="slds-button slds-button--brand" onclick="{!c.saveAndClose}">Save and Close</button>
                    </div>
                </div>
            </div>
            <div class="slds-backdrop slds-backdrop--open"></div>
            <!--###### MODAL BOX Part END Here ######-->
        </aura:if>
    </div>
    
    <!--Modal Popup for opening existing day record (Existing Travel Expense Record) -->
    <div class="slds-m-around--xx-large">    
        <!--Use aura:if tag to display Model Box, on the bese of conditions. [isOpen boolean attribute] -->   
        <aura:if isTrue="{!v.isExistingDayOpen}">
            
            <!--###### MODAL BOX Start From Here ######--> 
            <div role="dialog" aura:id="Modalbox" aria-labelledby="header99" class="slds-modal slds-fade-in-open" style="margin-top:120px;"> <!--  style="margin-top:120px;"-->
                <div class="slds-modal__container">
                    <!-- ###### MODAL BOX HEADER Part Start From Here ######-->
                    <div class="slds-modal__header">
                        <button class="slds-button slds-modal__close slds-button--icon-inverse" title="Close" onclick="{!c.closeModel}">
                            X
                            <span class="slds-assistive-text">Close</span>
                        </button>
                        <h2 id="header99" class="slds-text-heading--medium" style="font-weight:bold;">{!v.day_header}
                        </h2>
                    </div>
                    <!--###### MODAL BOX BODY Part Start From Here ######-->
                    <div class="slds-modal__content slds-p-around--medium">
                        
                        <div class="slds">
                            <div class="slds-grid slds-wrap slds-grid--pull-padded" style="background:#fff; margin:0px; padding:0 0 10px 0; border:0px solid #cce4f8;">
                                
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            
                                            <ui:inputDate class="slds-input" 
                                                          aura:id="dateId" 
                                                          format="dd/MM/yyyy" 
                                                          label="Date"
                                                          displayDatePicker="true" 
                                                          value="{!v.travelexpense.Date__c}" 
                                                          required="true" 
                                                          disabled="true" />             
                                        </div>
                                    </div>
                                </div>
                                
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <ui:inputSelect class="slds-input" 
                                                            label="Items" 
                                                            aura:id="items" 
                                                            value="{!v.travelexpense.Item__c}" 
                                                            change="{!c.onPicklistChange}"
                                                            required="true" 
                                                            disabled="false"
                                                            />     
                                        </div>
                                        
                                    </div>
                                </div>
                                
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <!--<lightning:input type="text" label="No Of Farmers Invited" name="date" required="true"/>-->
                                            <ui:inputNumber class="slds-input" 
                                                            aura:id="amount" 
                                                            label="Amount" 
                                                            format="##.00"
                                                            blur="{!c.onChange}"
                                                            value="{!v.travelexpense.Amount__c}"
                                                            required="true"/>            
                                        </div>
                                    </div>
                                </div>
                                
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <!--<lightning:input type="text" label="No Of R1 Invited" name="date" required="true"/>-->
                                            <ui:inputText class="slds-input" 
                                                          aura:id="purpose" 
                                                          label="Purpose" 
                                                          value="{!v.travelexpense.Purpose__c}" 
                                                          required="true" />         
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <!--<lightning:input type="text" label="No Of R1 Invited" name="date" required="true"/>-->
                                            <ui:inputText class="slds-input" 
                                                          aura:id="location" 
                                                          label="Location" 
                                                          value="{!v.travelexpense.Location__c}" 
                                                          required="true" />         
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-3" style="margin-top:12px;">
                                    <div class="slds-form-element">
                                        <div class="slds-form-element__control">
                                            <!--<lightning:input type="text" label="No Of R1 Invited" name="date" required="true"/>-->
                                            <ui:inputText class="slds-input" 
                                                          aura:id="remark" 
                                                          label="Remark" 
                                                          value="{!v.travelexpense.Remarks__c}" 
                                                          required="true" />         
                                        </div>
                                    </div>
                                </div>
                                <div style="display:none;">
                                    <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-2" style="margin-top:12px;">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <!--<lightning:input type="text" label="No Of R1 Invited" name="date" required="true"/>-->
                                                <ui:inputText class="slds-input" 
                                                              aura:id="monthId" 
                                                              value="{!v.travelexpense.Month__c}"  />         
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                
                            </div>
                        </div>           
                        
                        
                    </div>
                    <!--###### MODAL BOX FOOTER Part Start From Here ######-->
                    <div class="slds-modal__footer">
                        <button class="slds-button slds-button--neutral" onclick="{!c.closeModel}" >Cancel</button>
                        
                        <button class="slds-button slds-button--brand" onclick="{!c.saveAndCloseExistDay}">Save and Close</button>
                    </div>
                </div>
            </div>
            <div class="slds-backdrop slds-backdrop--open"></div>
            <!--###### MODAL BOX Part END Here ######-->
        </aura:if>
    </div>
    
    
    <!--Modal Popup for delet Month Record -->
    <div class="slds-m-around--xx-large">    
        <!--Use aura:if tag to display Model Box, on the bese of conditions. [isOpenMonth boolean attribute] -->   
        <aura:if isTrue="{!v.isOpenDelete}">
            
            <!--###### MODAL BOX Start From Here ######--> 
            <div role="dialog" aura:id="Modalbox" aria-labelledby="header99" class="slds-modal slds-fade-in-open" style="margin-top:80px;margin-left:350px;margin-right:350px;"> <!--  style="margin-top:120px;"-->
                <div class="slds-modal__container">
                    <!-- ###### MODAL BOX HEADER Part Start From Here ######-->
                    <div class="slds-modal__header">
                        <button class="slds-button slds-modal__close slds-button--icon-inverse" title="Close" onclick="{!c.closeDeleteModel}">
                            X
                            <span class="slds-assistive-text">Close</span>
                        </button>
                        <h2 id="header99" class="slds-text-heading--medium" style="font-weight:bold;">
                            {!v.delete_header}
                        </h2>
                    </div>
                    <!--###### MODAL BOX BODY Part Start From Here ######-->
                    
                    <!--###### MODAL BOX BODY Part Start From Here ######-->
                    <div class="slds-modal__content slds-p-around--medium">
                        <!--<h2 class="slds-align_absolute-center"><strong><span id="DeleteActivityId">#{!v.recordNo}</span></strong></h2>-->
                        <h4 class="slds-align_absolute-center">Are you sure you want to delete this month record?</h4>
                    </div>
                    <!--###### MODAL BOX FOOTER Part Start From Here ######-->
                    <div class="slds-modal__footer">
                        <button class="slds-button slds-button--neutral" onclick="{!c.closeDeleteModel}" >Cancel</button>
                        <button class="slds-button slds-button--brand" onclick="{!c.clickDelete}" >Delete</button>
                    </div>
                </div>
            </div>
            <div class="slds-backdrop slds-backdrop--open"></div>
            <!--###### MODAL BOX Part END Here ######-->
        </aura:if>
    </div>
    
    <!--Modal Popup for delete Day Record -->
    <div class="slds-m-around--xx-large">    
        <!--Use aura:if tag to display Model Box, on the bese of conditions. [isOpenMonth boolean attribute] -->   
        <aura:if isTrue="{!v.isOpenDeleteDay}">
            
            <!--###### MODAL BOX Start From Here ######--> 
            <div role="dialog" aura:id="Modalbox" aria-labelledby="header99" class="slds-modal slds-fade-in-open" style="margin-top:80px;margin-left:350px;margin-right:350px;"> <!--  style="margin-top:120px;"-->
                <div class="slds-modal__container">
                    <!-- ###### MODAL BOX HEADER Part Start From Here ######-->
                    <div class="slds-modal__header">
