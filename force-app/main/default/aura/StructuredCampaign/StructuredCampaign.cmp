            </div>
        </aura:if>  
    
        <aura:if isTrue="{!v.isOpenSD}">     
            <!--###### MODAL BOX Start From Here ######--> 
            <div aura:id="lookupmodal" class="slds-modal slds-fade-in-open slds-modal--large"  aria-hidden="false" role="dialog">
                <div class="slds-modal__container">
                    <!-- ###### MODAL BOX HEADER Part Start From Here ######-->
                    <div class="slds-modal__header">
                        <button class="slds-button slds-modal__close slds-button--icon-inverse"
                                title="Close" onclick="{!c.closeModel}">
                            X
                            <span class="slds-assistive-text">{!$Label.c.Close}</span>
                        </button>
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">{!v.modalHeader}</h2>                        
                    </div>
                    <!--###### MODAL BOX BODY Part Start From Here ######-->
                    <div class="slds-modal__content slds-p-around--medium">
                        <div aura:id="accountdata" style="display:all;">
                            <ldt:datatableDev title="{!$Label.c.Region}" aura:id="accountTable"
                                              dataRows="{!v.accountList}" header="{!v.accountColumns}"
                                              config="{!v.accountTableConfig}"/>
                        </div>   
                    </div>
                    <!--###### MODAL BOX FOOTER Part Start From Here ######-->
                    <div class="slds-modal__footer">
                        <button class="slds-button slds-button--neutral" 
                                onclick="{!c.closeModel}" >{!$Label.c.Cancel}</button>
                    </div>
                </div>
            </div>
            <div aura:id="backdrop" class="slds-backdrop slds-backdrop--open tog"></div>
            <!--###### MODAL BOX Part END Here ######-->
        </aura:if>
        
