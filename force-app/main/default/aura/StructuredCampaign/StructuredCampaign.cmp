            <aura:if isTrue="{!v.isEdit}">
                <div class="slds-p-horizontal--small slds-size--1-of-1 slds-medium-size--1-of-2 slds-large-size--1-of-4" >
            <div class="slds-form-element" style="margin-top:12px;">
