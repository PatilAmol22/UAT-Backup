<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>20011247@upl-ltd.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Step 1</label>
        <name>Step_1</name>
    </approvalStep>
    <emailTemplate>unfiled$public/Fr_Contract_Management_Approval_Process</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Contract_management__c.Contract_Attached__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contract_management__c.Status__c</field>
            <operation>equals</operation>
            <value>Open</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Commercial_Manager_ApprovedResponse_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>CommercialManager_Approv_Response_Status</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>France_Contract_SM_Approval_Mail</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Status_SM_Approved</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Commercial_Manager_Reject_Response_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>CommercialManager_Reject_Response_Status</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>France_Contract_SM_Rejection_Mail</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Status_SM_Not_Approved</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>France SM Contract Approval</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
